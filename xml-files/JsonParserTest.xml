<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite1_JsonParserTest" verbose="1" allow-return-values="true">

    <test name="JsonParserTest1">
        <parameter name="pathName"  value="src/main/resources/andrew-cart1.json"/>
        <groups>
            <run>
                <exclude name="readFromFile_NotNull"></exclude>
            </run>
        </groups>
        <classes>
            <class name="parser.JsonParserTest"></class>
        </classes>
    </test>

    <test name="JsonParserTest2">
        <parameter name="pathName"  value="src/main/resources/andrew-cart2.json"/>
        <groups>
            <run>
                <exclude name="writeToFile"></exclude>
            </run>
        </groups>
        <classes>
            <class name="parser.JsonParserTest"></class>
        </classes>
    </test>

    <test name="JsonParserTest3">
        <parameter name="pathName"  value="src/main/resources/eugen-cart3.json"/>
            <groups>
                 <run>
                        <exclude name="writeToFile"></exclude>
                        <exclude name="readFromFile_NotNull"></exclude>
                 </run>
            </groups>
        <classes>
            <class name="parser.JsonParserTest"></class>
        </classes>
    </test>

    <test name="JsonParserTest4">
        <parameter name="pathName"  value="src/main/resources/eugen-cart4.json"/>
        <classes>
            <class name="parser.JsonParserTest"></class>
        </classes>
    </test>

    <test name="JsonParserTest5">
        <parameter name="pathName"  value="src/main/resources/eugen-cart.json"/>
        <classes>
            <class name="parser.JsonParserTest"></class>
        </classes>
    </test>
</suite>